

{
  "env": {
    "dev": {
      "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "@babel/preset-typescript"
      ],
      "plugins": [
          [
              "@babel/plugin-transform-react-jsx",
              {
                  "runtime": "automatic"
              },
              "@babel-plugin-styled-components"
          ]
      ]
    },
    "production": {
      "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "@babel/preset-typescript"
      ],
      "plugins": [
          [
              "@babel/plugin-transform-react-jsx",
              {
                  "runtime": "automatic"
              },
              "@babel-plugin-styled-components"
          ]
      ]
    }
  }
}